{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "composite": true
  },
  "include": ["index.ts"]
}
